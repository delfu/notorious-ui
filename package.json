{
  "name": "notorious-ui",
  "version": "0.1.1",
  "license": "ISC",
  "author": {
    "name": "Delong Fu"
  },
  "description": "A small neumorphic UI components library",
  "repository": {
    "url": "https://github.com/delfu/notorious-ui"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^16.14.0",
    "react-icons": "^4.2.0",
    "slate": "^0.57.1",
    "slate-history": "^0.57.1",
    "slate-react": "^0.57.1",
    "storybook": "^6.0.21",
    "styled-components": "^5.2.0",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-storysource": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/node-logger": "^6.3.8",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.8",
    "@types/react": "^16.9.49",
    "@types/styled-components": "^5.1.3"
  },
  "scripts": {
    "build": "tsc",
    "create-component": "./scripts/create-component",
    "rename-component": "./scripts/rename-component",
    "storybook": "start-storybook -p 6006 -s public",
    "prepublish": "tsc",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
